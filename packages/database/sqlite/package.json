    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@undb/authz": "workspace:^",
    "@undb/core": "^0.0.0",
    "@undb/domain": "^0.0.0",
    "@undb/integrations": "^0.0.0",
    "@undb/openapi": "workspace:^0.0.0",
    "date-fns": "^3.0.0",
    "lodash-es": "^4.17.21",
    "oxide.ts": "^1.1.0",
    "p-map": "^7.0.0",
    "reflect-metadata": "^0.2.1",
    "ts-mixer": "^6.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.9.5",
    "@mikro-orm/reflection": "^5.9.5",
    "@swc/core": "1.4.11",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^9.0.7",
    "@undb/logger": "^0.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsconfig": "^0.0.0",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "type-fest": "^4.8.3",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "vitest-mock-extended": "^1.3.1"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "main": "./dist/index",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts"
    ]
  },
  "private": true,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "coverage": "vitest run --coverage",
    "db:migration:create": "NODE_OPTIONS=\"--loader ts-node/esm\" mikro-orm migration:create",
    "db:schema:fresh": "NODE_OPTIONS=\"--loader ts-node/esm\" mikro-orm schema:fresh -r",
    "db:schema:update": "NODE_OPTIONS=\"--loader ts-node/esm\" mikro-orm schema:update -r",
    "dev": "tsup --watch",
    "prebuild": "pnpm run clean",
    "test": "TZ=UTC vitest run",
    "test:watch": "TZ=UTC vitest"
  },
  "type": "module"
}
